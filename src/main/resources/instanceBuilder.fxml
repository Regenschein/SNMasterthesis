<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane fx:id="aP_main" prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.InstanceBuilderController">
   <children>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0">
         <children>
            <TextField fx:id="tF_subjectName" />
            <TableView fx:id="tV_newInstance" editable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
              <columns>
                <TableColumn fx:id="tC_newInstancePredicate" maxWidth="1.7976931348623157E308" minWidth="100.0" prefWidth="145.0" text="predicate" />
                  <TableColumn fx:id="tC_newInstanceObject" maxWidth="1.7976931348623157E308" minWidth="100.0" onEditCancel="#changeCellEvent" onEditCommit="#changeCellEvent" prefWidth="145.0" text="object" />
              </columns>
            </TableView>
            <Button fx:id="btn_addNode" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#addNode" text="Add Node" />
         </children>
      </VBox>
   </children>
</AnchorPane>
